check if user can sudo
	CAN_I_RUN_SUDO=$(sudo -n uptime 2>&1|grep "load"|wc -l)
	if [ ${CAN_I_RUN_SUDO} -gt 0 ] then
  		echo "I can run the sudo command"
    	else
        	echo "I can't run the Sudo command"
	fi

